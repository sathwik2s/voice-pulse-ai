{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/(style.css|api.js|dashboard.js|recorder.js)", "destination": "/frontend/$1" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
